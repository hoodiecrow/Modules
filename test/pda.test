test pda-2.0 {} -setup {
    set res {}
    PDA create M [dict create Q {q0 q1 q2 q3 q4 q5 q6} Σ {sp w n} Γ {S} δ {} s q1 Z S F {q0 q6}]
    M addTransition {q[1-5]} nl S {q1 {}    {}}
    M addTransition q1       {} S {q0 {}    {emit success "no input"}}
    M addTransition q1       sp S {q1 {}    {}}
    M addTransition {q[12]}  w  S {q2 {}    {emit cmd [lindex $token 1]}}
    M addTransition {q[23]}  sp S {q3 {}    {}}
    M addTransition {q[345]} w  S {q4 {}    {emit arg [lindex $token 1]}}
    M addTransition {q[45]}  sp S {q5 {}    {}}
    M addTransition {q[2-5]} {} S {q6 {}    {emit success match}}
} -body {
    M run {}
    lappend res [M output]
    M reset
    M run {sp sp {w foo} sp}
    lappend res [M output]
    M reset
    M run {sp {w foo} sp sp {w bar} sp}
    lappend res [M output]
} -cleanup {
    M destroy
    log::lvSuppressLE i 1
} -result {{success {no input}} {cmd foo success match} {cmd foo arg bar success match}}


